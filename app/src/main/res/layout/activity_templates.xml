<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#9BBECC"
    tools:context=".Templates">

    <ImageButton
        android:id="@+id/Template2"
        android:layout_width="190dp"
        android:layout_height="225dp"
        android:background="#E4E3E3"
        android:contentDescription="Template2"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.443"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.968"
        app:srcCompat="@drawable/template2" />

    <ImageButton
        android:id="@+id/Template1"
        android:layout_width="190dp"
        android:layout_height="225dp"
        android:background="#E4E3E3"
        android:contentDescription="Template1"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.031"
        app:srcCompat="@drawable/template1" />

    <ImageButton
        android:id="@+id/Template3"
        android:layout_width="190dp"
        android:layout_height="225dp"
        android:background="#E4E3E3"
        android:contentDescription="Template6"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.443"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/template6" />

</androidx.constraintlayout.widget.ConstraintLayout>